{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "allowJs": true,
    "strict": true,
    "esModuleInterop": true,                            
    "skipLibCheck": true,                                 
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist"
  },
  "exclude": ["node_modules", "dist", "coverage"],
  "include": ["src"]
}
